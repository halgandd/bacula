@@out@ nul
messages
@@out@ tmp/log1.out
@# Force Incremental on the second Volume
update volume=TestVolume001 VolStatus=Used
status storage=DDS-4
@#setdebug level=120 storage=DDS-4
run level=Incremental job=NightlySave yes
wait
list volumes
status storage=DDS-4
