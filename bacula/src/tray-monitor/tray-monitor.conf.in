#
# Bacula Tray Monitor Configuration File
#

Monitor {
  Name = @hostname@-dir
}
   
Client {
  Name = @hostname@-fd
  Address = @hostname@
  FDPort = @fd_port@
  Password = "@fd_password@"          # password for FileDaemon
}

Storage {
  Name = @hostname@-sd
  Address = @hostname@
  SDPort = @sd_port@
  Password = "@sd_password@"          # password for StorageDaemon
}